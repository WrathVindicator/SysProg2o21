#include <unistd.h>

#include <stdio.h>

#include <stdlib.h>


int main() {

    pid_t pid; // new pid


    pid = fork(); 


    if (pid > 0) 

        exit(0);


    chdir("/"); 


    if (setsid() < 0) 

        exit(0);


    printf("%d\n", getpid());


    close(0); 

    close(1);

    close(2);

  

               

    return 0;

}
